<SCENE>

<INITIALS>
    <frustum near="0.1" far="500.0"/>
    <translation x="0" y="0" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" /> 
    <scale sx="1" sy="1" sz="1" />
    <reference length="0" />
</INITIALS>

<ILLUMINATION>
    <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
    <background r="0.63" g="0.56" b="0.35" a="1.0" />
</ILLUMINATION>

<LIGHTS>
    <LIGHT id="luz_cena">
        <enable value= "1" />
        <position x="10" y="7" z="10" w="1.0" />
        <ambient r="0.9" g="0.9" b="0.9" a="1.0" />
        <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
        <specular r="0.9" g="0.9" b="0.9" a="1.0" /> 
    </LIGHT>
</LIGHTS>


<TEXTURES>
    <TEXTURE id="whiteCircle">
        <file path="textures/whitecircle.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="whiteSquare">
        <file path="textures/whitesquare.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="blackCircle">
        <file path="textures/blackcircle.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="blackSquare">
        <file path="textures/blacksquare.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="lightwood">
        <file path="textures/wood_light.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="wood">
        <file path="textures/wood.png" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>
</TEXTURES>


<MATERIALS>
     <MATERIAL id="branco">                               
        <shininess value="1.0" />
        <specular r="0" g="0" b="0" a="1" />        <!-- specular reflection -->
        <diffuse r="0.6" g="0.6" b="0.6" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.6" g="0.6" b="0.6" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>


</MATERIALS>


<LEAVES>
	<LEAF id="square" type="rectangle" args="-0.5 0.5 0.5 -0.5" />
	<LEAF id="cylinder" type="cylinder" args="1.0 0.5 0.5 20 20" /> 
</LEAVES>


<NODES>
    <ROOT id="root" />

    <NODE id="root">
    	<MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
        	<DESCENDANT id="blackcircle" />
        	<DESCENDANT id="blacksquare" />
        	<DESCENDANT id="whitecircle" />
        	<DESCENDANT id="whitesquare" />
        	<DESCENDANT id="blacktower" />
        	<DESCENDANT id="whitetower" />
        </DESCENDANTS>
    </NODE>

	 <!-- CUBO -->

    <NODE id="cubesquareFront">

        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubesquareBack">


        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="-0.5" />
        <ROTATION axis="x" angle="180" />


        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubesquareLeft">

        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0.5" y="0" z="0" />
        <ROTATION axis="y" angle="90" />

        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubesquareRight">


        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="-0.5" y="0" z="0" />
        <ROTATION axis="y" angle="-90" />

        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubesquareBottom">


        <MATERIAL id="null" />
        <TEXTURE id="null" />
        <TRANSLATION x="0" y="-0.5" z="0" />
        <ROTATION axis="x" angle="90" />


        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubesquareTop">


        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0.5" z="0" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <DESCENDANT id="square" />
        </DESCENDANTS>

    </NODE>

     <NODE id="cubewithoutcover">
        <MATERIAL id="null" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <DESCENDANT id="cubesquareBottom" /> 
            <DESCENDANT id="cubesquareLeft" /> 
            <DESCENDANT id="cubesquareRight" /> 
            <DESCENDANT id="cubesquareFront" /> 
            <DESCENDANT id="cubesquareBack" />  
        </DESCENDANTS>
    </NODE>

     <NODE id="cubewithoutcoverlightwood">
        <MATERIAL id="branco" />
        <TEXTURE id="lightwood" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcover" />
        </DESCENDANTS>
    </NODE>

    <NODE id="cubewithoutcoverwood">
        <MATERIAL id="branco" />
        <TEXTURE id="wood" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcover" />
        </DESCENDANTS>
    </NODE>


    <NODE id="blacktower">
    	<MATERIAL id="branco" />
        <TEXTURE id="wood" />

        <TRANSLATION x="0" y="0.75" z="0" />
        <SCALE sx="0.5" sy="1.5" sz="0.5" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcoverwood" />
            <DESCENDANT id="cubesquareTop" />
        </DESCENDANTS>
    </NODE>

    <NODE id="whitetower">
    	<MATERIAL id="branco" />
        <TEXTURE id="lightwood" />

        <TRANSLATION x="0" y="0.75" z="0" />
        <SCALE sx="0.5" sy="1.5" sz="0.5" />

        <DESCENDANTS>
            <DESCENDANT id="cylinder" />
        </DESCENDANTS>
    </NODE>

    <NODE id="blacksquare">
        <MATERIAL id="branco" />
        <TEXTURE id="blackSquare" />

        <SCALE sx="1" sy="0.2" sz="1" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcoverlightwood" />
            <DESCENDANT id="cubesquareTop" /> 
        </DESCENDANTS>
    </NODE>

    <NODE id="whitesquare">
        <MATERIAL id="branco" />
        <TEXTURE id="whiteSquare" />

        <TRANSLATION x="1" y="0" z="0" />
        <SCALE sx="1" sy="0.2" sz="1" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcoverlightwood" />
            <DESCENDANT id="cubesquareTop" /> 
        </DESCENDANTS>
    </NODE>

    <NODE id="blackcircle">
        <MATERIAL id="branco" />
        <TEXTURE id="blackCircle" />

        <TRANSLATION x="2" y="0" z="0" />
        <SCALE sx="1" sy="0.2" sz="1" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcoverlightwood" />
            <DESCENDANT id="cubesquareTop" /> 
        </DESCENDANTS>
    </NODE>

    <NODE id="whitecircle">
        <MATERIAL id="branco" />
        <TEXTURE id="whiteCircle" />

        <TRANSLATION x="3" y="0" z="0" />
        <SCALE sx="1" sy="0.2" sz="1" />

        <DESCENDANTS>
            <DESCENDANT id="cubewithoutcoverlightwood" />
            <DESCENDANT id="cubesquareTop" /> 
        </DESCENDANTS>
    </NODE>


    <!-- FIM CUBO -->

</NODES>

<ANIMATIONS>
	<ANIMATION id="aguardar" span="5" type="linear">
		<CONTROLPOINT xx="0" yy="0" zz="0" />
	</ANIMATION>
    <ANIMATION id="descolagem" span="5" type="linear">
		<CONTROLPOINT xx="0" yy="0" zz="0" />
        <CONTROLPOINT xx="10" yy="0" zz="0" />
    </ANIMATION>
	<ANIMATION id="orientacao" span="5" type="circular" center="10 0 5" radius="5" startang="90" rotang="-90"/>
	<ANIMATION id="orbita" span="10" type="linear">
		<CONTROLPOINT xx="15" yy="00" zz="5" />
		<CONTROLPOINT xx="15" yy="0" zz="20" />
	</ANIMATION>
	<ANIMATION id="afastamento" span="5" type="linear">
		<CONTROLPOINT xx="0" yy="0" zz="0" />
		<CONTROLPOINT xx="0" yy="-5" zz="0" />
	</ANIMATION>
</ANIMATIONS>

</SCENE>